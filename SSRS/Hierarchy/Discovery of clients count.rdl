  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select  ad.AgentName [Discovery Method],
count(case when sys.Client0=1 then '*' else NULL end) [Client Installed],
count(case when sys.active0 =1 then '*' else NULL end) [Active Clients],
count(case when sys.Client0 is NULL then '*' else NULL end) [Missing Clients],
count(case when sys.Obsolete0= 1 then '*' else NULL end) [Obsolete Clients]

from v_R_System sys, v_AgentDiscoveries AD 
where AD.ResourceId=sys.ResourceID and
--sys.Client0 is NULL and sys.name0 not like '%sms%'
--and DATEDIFF(dd,AD.AgentTime,GetDate()) &lt; 30
sys.Distinguished_Name0 not like '%OTHER%'
--and ad.AgentName='SMS_AD_SYSTEM_DISCOVERY_AGENT'
group by ad.AgentName
--order by sys.name0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Discovery_Method">
          <DataField>Discovery Method</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Client_Installed">
          <DataField>Client Installed</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Active_Clients">
          <DataField>Active Clients</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Missing_Clients">
          <DataField>Missing Clients</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Obsolete_Clients">
          <DataField>Obsolete Clients</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
