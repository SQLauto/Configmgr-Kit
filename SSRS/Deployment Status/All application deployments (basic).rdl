    <DataSet Name="FirstMatchingAppOrCollection">
      <Fields>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="LocName">
          <Value>=SrsResources.Localization.GetDBString(Fields!Name.Value, User!Language, "SMS_Collection-Name")</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>if @ChooseBy = 'Collection' 
begin 
select
  Name,
  ID
from
(
  select 
    @AllParameterValue as Name, 
    'All' as ID 
  union all 
  select distinct 
    DS.CollectionName as Name, 
    DS.CollectionID as ID 
  from fn_rbac_DeploymentSummary(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) DS 
  where DS.CollectionName like @SecondParameter and FeatureType=1 
) temp
order by (case temp.Name when @AllParameterValue then 0 else 1 end), temp.Name
end 
else if @ChooseBy = 'Application' 
begin 
select
  Name,
  ID
from
(
  select 
    @AllParameterValue as Name, 
    0 as ID 
  union all 
  select distinct 
    DS.SoftwareName as Name, 
    DS.CI_ID as ID 
  from fn_rbac_DeploymentSummary(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) DS 
  where DS.SoftwareName like @SecondParameter and FeatureType=1
) temp
order by (case temp.Name when @AllParameterValue then 0 else 1 end), temp.Name
end</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ChooseBy">
            <Value>=Parameters!ChooseBy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SecondParameter">
            <Value>=Parameters!SecondParameter.Value</Value>
          </QueryParameter>
