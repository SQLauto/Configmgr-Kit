      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>DECLARE	@CollectionVar nvarchar(64) 
DECLARE @ApplicationVar nvarchar(64) 

IF @ChooseBy = 'Collection' 
BEGIN 
  SET @CollectionVar = @ThirdParameter 
  SET @ApplicationVar = @FourthParameter 
END 
ELSE IF @ChooseBy = 'Application' 
BEGIN 
  SET @CollectionVar = @FourthParameter 
  SET @ApplicationVar = @ThirdParameter 
END 

SELECT DISTINCT 
  CIS.CIVersion as Version, 
  DSC.CollectionID, 
  DSC.DeploymentID, 
  DSC.CI_ID, 
  DSC.AssignmentID, 
  DSC.CollectionName, 
  DSC.SoftwareName, 
  DSC.DeploymentTime, 
  DSC.ModificationTime,
  AA.LastModifiedBy,
  DSC.FeatureType, 
  DSC.DeploymentIntent, 
  DSC.NumberSuccess, 
  DSC.NumberInProgress, 
  DSC.NumberUnknown, 
  DSC.NumberErrors, 
  DSC.NumberOther, 
  apptypes.typeId as ApplicationType 
FROM fn_rbac_DeploymentSummary(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) DSC 
JOIN fn_rbac_ListApplicationCIs(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) CIS ON DSC.CI_ID = CIS.CI_ID
JOIN fn_rbac_ApplicationAssignment(@UserSIDs)  AA on AA.AssignmentID = DSC.AssignmentID
JOIN
  ( SELECT 1 as Id, 'ApplicationType1' as typeId union all
    SELECT 2 as Id, 'ApplicationType2' as typeId union all
    SELECT 3 as Id, 'ApplicationType3' as typeId union all
    SELECT 4 as Id, 'ApplicationType4' as typeId union all
    SELECT 5 as Id, 'ApplicationType5' as typeId union all
    SELECT 6 as Id, 'ApplicationType6' as typeId union all
    SELECT 7 as Id, 'ApplicationType7' as typeId ) apptypes on apptypes.Id = DSC.FeatureType
WHERE DATEADD(d, CAST(@TimeFrame AS Integer), DSC.DeploymentTime) &gt;= GETDATE() 
AND 
isnull((DSC.NumberSuccess + DSC.NumberOther) * 100 / nullif(DSC.NumberSuccess + DSC.NumberInProgress + DSC.NumberErrors + DSC.NumberOther + DSC.NumberUnknown, 0), 0) &gt;= @SuccessPct 
AND 
(
  @CollectionVar = 'All' 
  OR DSC.CollectionID = @CollectionVar 
  OR @CollectionVar = ''
)
AND
(  @Administrator = 'All' OR AA.LastModifiedBy = @Administrator )
AND 
(
  @ApplicationVar = 0 
  OR DSC.CI_ID LIKE @ApplicationVar 
  OR @ApplicationVar = ''
  )</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@TimeFrame">
            <Value>=Parameters!TimeFrame.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ChooseBy">
            <Value>=Parameters!ChooseBy.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ThirdParameter">
            <Value>=Parameters!ThirdParameter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SuccessPct">
            <Value>=Parameters!SuccessPct.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@locale">
            <Value>=Parameters!locale.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@FourthParameter">
            <Value>=Parameters!FourthParameter.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Administrator">
            <Value>=Parameters!Administrator.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
