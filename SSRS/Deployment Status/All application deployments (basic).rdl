    <DataSet Name="LastModifiedByDataset">
      <Fields>
        <Field Name="LastModifiedBy">
          <DataField>LastModifiedBy</DataField>
          <rd:TypeName>System.Object</rd:TypeName>
        </Field>
        <Field Name="ID">
          <DataField>ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>if @ChooseBy = 'Collection' 
begin 
select @AllParameterValue as LastModifiedBy, 'All' as ID
union all
select distinct aa.LastModifiedBy, aa.LastModifiedBy as ID
from fn_rbac_ApplicationAssignment(@UserSIDs)  aa
join fn_rbac_DeploymentSummary(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) dsc
on aa.AssignmentID = dsc.AssignmentID
where (@ThirdParameter = 'All' OR dsc.CollectionID = @ThirdParameter) and (@FourthParameter = 0 OR dsc.CI_ID = @FourthParameter)
order by LastModifiedBy
end 
else if @ChooseBy = 'Application' 
begin 
select @AllParameterValue as LastModifiedBy, 'All' as ID
union all
select distinct aa.LastModifiedBy, aa.LastModifiedBy as ID
from fn_rbac_ApplicationAssignment(@UserSIDs)  aa
join fn_rbac_DeploymentSummary(dbo.fn_LShortNameToLCID(@locale), @UserSIDs) dsc
on aa.AssignmentID = dsc.AssignmentID
where (@ThirdParameter = 0 OR dsc.CI_ID = @ThirdParameter) and (@FourthParameter = 'All' OR dsc.CollectionID = @FourthParameter)
order by LastModifiedBy
end</CommandText>
        <QueryParameters>
