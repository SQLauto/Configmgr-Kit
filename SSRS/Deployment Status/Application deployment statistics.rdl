        </Field>
        <Field Name="TotalDeployments">
          <Value>=Fields!NumberInstalled.Value + Fields!NumberInProgress.Value + Fields!NumberErrors.Value + Fields!NumberUnknown.Value + Fields!NumberReqsNotMet.Value</Value>
        </Field>
        <Field Name="PctInstalled">
          <Value>=iif(Fields!TotalDeployments.Value &gt; 0, Fields!NumberInstalled.Value / Fields!TotalDeployments.Value, 0)</Value>
        </Field>
        <Field Name="PctInProgress">
          <Value>=iif(Fields!TotalDeployments.Value &gt; 0, Fields!NumberInProgress.Value / Fields!TotalDeployments.Value, 0)</Value>
        </Field>
        <Field Name="PctErrors">
          <Value>=iif(Fields!TotalDeployments.Value &gt; 0, Fields!NumberErrors.Value / Fields!TotalDeployments.Value, 0)</Value>
        </Field>
        <Field Name="PctUnknown">
          <Value>=iif(Fields!TotalDeployments.Value &gt; 0, Fields!NumberUnknown.Value / Fields!TotalDeployments.Value, 0)</Value>
        </Field>
        <Field Name="PctReqsNotMet">
          <Value>=iif(Fields!TotalDeployments.Value &gt; 0, Fields!NumberReqsNotMet.Value / Fields!TotalDeployments.Value, 0)</Value>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>SELECT
  DS.TargetCollectionID as CollectionID,
  DS.ParentID as AppCI,
  DS.AssignmentID,
  co.Name as CollectionName,
  DS.Descript as SoftwareName,
  DS.DeploymentTime,
  DS.ModificationTime,
  DS.Success as NumberInstalled,
  DS.InProgress as NumberInProgress,
  DS.Unknown as NumberUnknown,
  DS.Error as NumberErrors,
  DS.RequirementsNotMet as NumberReqsNotMet
FROM
  fn_rbac_AppDeploymentSummary(@UserSIDs)   DS
JOIN fn_rbac_Collection(@UserSIDs)  co ON DS.TargetCollectionID = co.CollectionID
WHERE DS.ParentID = @AppCI
AND DS.TargetCollectionID = @CollectionID
AND DS.ParentID = DS.CI_ID
AND DS.AssignmentID = @AssignmentID</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AppCI">
            <Value>=Parameters!AppCI.Value</Value>
