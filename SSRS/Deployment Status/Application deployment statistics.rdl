          <QueryParameter Name="@AssignmentID">
            <Value>=Parameters!AssignmentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
