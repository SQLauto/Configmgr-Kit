      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>SELECT 
  SUM(DSS.Success) as NumberInstalled, 
  SUM(DSS.InProgress) as NumberInProgress, 
  SUM(DSS.Error) as NumberErrors, 
  SUM(DSS.AlreadyPresent) as NumberAlreadyPresent, 
  SUM(DSS.RequirementsNotMet) as NumberReqsNotMet
from
  fn_rbac_AppDTDeploymentSummary(@UserSIDs)   DSS
where 
  DSS.ParentID = @AppCI AND
  DSS.TargetCollectionID = @CollectionID AND
  DSS.AssignmentID = @AssignmentID</CommandText>
        <QueryParameters>
