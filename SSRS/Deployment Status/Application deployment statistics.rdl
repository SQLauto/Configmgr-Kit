        </Field>
        <Field Name="SoftwareName">
          <DataField>SoftwareName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="CollectionID">
          <DataField>CollectionID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AppCI">
          <DataField>AppCI</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="StatusType">
          <DataField>StatusType</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AssignmentID">
          <DataField>AssignmentID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ParentID">
          <DataField>ParentID</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="AppStatusType">
          <DataField>AppStatusType</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>ID5C6358F24BB64a1bA16E8D96795D8602</DataSourceName>
        <CommandText>SELECT DTCI, SoftwareName, CollectionID, AppCI, AssignmentID, ParentID, name, count,
CASE WHEN name = 'DTInstalled' THEN 1
     WHEN name = 'DTAlreadyPresent' THEN 0
     WHEN name = 'DTInProgress' THEN 2
     WHEN name = 'DTErrors' THEN 5
     WHEN name = 'DTReqsNotMet' THEN 3
     END AS StatusType,
CASE WHEN name = 'DTInstalled' or name = 'DTAlreadyPresent' THEN 1
     WHEN name = 'DTInProgress' THEN 2
     WHEN name = 'DTErrors' THEN 5
     WHEN name = 'DTReqsNotMet' THEN 3 
     END AS AppStatusType
FROM
(
SELECT 
  DS.Success DTInstalled,
  DS.InProgress DTInProgress,
  DS.AlreadyPresent DTAlreadyPresent,
  DS.Error DTErrors,
  DS.RequirementsNotMet DTReqsNotMet,
  DS.CI_ID AS DTCI, 
  DS.Descript AS SoftwareName, 
  DS.TargetCollectionID AS CollectionID, 
  DS.CI_ID AS AppCI,
  DS.AssignmentID,
  DS.ParentID
FROM fn_rbac_AppDTDeploymentSummary(@UserSIDs)  AS DS
WHERE DS.ParentID = @AppCI
AND DS.TargetCollectionID = @CollectionID
AND DS.AssignmentID = @AssignmentID
) a
UNPIVOT 
(
count FOR name IN (DTInstalled, DTInProgress, DTAlreadyPresent, DTErrors, DTReqsNotMet)
) AS b</CommandText>
        <QueryParameters>
          <QueryParameter Name="@UserSIDs">
            <Value>=Parameters!UserSIDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AppCI">
            <Value>=Parameters!AppCI.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@CollectionID">
            <Value>=Parameters!CollectionID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@AssignmentID">
            <Value>=Parameters!AssignmentID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
