  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>CM12</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AppName">
            <Value>=Parameters!AppName.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>declare @Total int
              declare @Accepted int

              select @Total=count(distinct machineID), @Accepted=sum(case ads.StatusType when 0 then 0 else 1 end)
              from fn_AppDeploymentAssetDetails(1033) ADS
			  where ads.AppName=@AppName
              --and ADS.CollectionName='SU_CM_San_Test_Computers'

select 
case ADS.Statustype
 when '1' then 'Success'
 when '2' then 'InProgress'
 when '3' then 'RequirementsNotMet'
 when '4' then 'Unknown'
 when '5' then 'Error'
Else 'NULL'
 end as 'Status of Targeted Resources',
Count(distinct ADS.MachineName) as Total,
 round((CAST(count(distinct ADS.machineID) as float)/(@Total) )*100,2) as 'Percentage of Resources'
--SUM (distinct CASE WHEN ADS.StatusType='1' THEN 1 ELSE 0 END) as float
--round((CAST(SUM (CASE WHEN ADS.StatusType='1' THEN 1 ELSE 0 END) as float)/COUNT(distinct ADS.MachineName ) )*100,2) as 'Success%',
--round((CAST(SUM (CASE WHEN ADS.StatusType='1' THEN 1 ELSE 0 END) as float)/COUNT(distinct ADS.MachineName ) )*100,2) as 'Success%'

 From fn_AppDeploymentAssetDetails(1033) ADS
 where ads.AppName=@AppName
 --ADS.DTName like 'UE-V 2.0 Patch for Excel' and
 --ADS.CollectionName='SU_CM_San_Test_Computers'
 group by ADS.StatusType</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Status_of_Targeted_Resources">
          <DataField>Status of Targeted Resources</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total">
          <DataField>Total</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Percentage_of_Resources">
          <DataField>Percentage of Resources</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Appname">
      <Query>
        <DataSourceName>CM12</DataSourceName>
        <CommandText>select SoftwareName From v_DeploymentSummary
where softwarename not like '%DEVEL%'
group by softwarename
order by 1</CommandText>
      </Query>
      <Fields>
        <Field Name="SoftwareName">
          <DataField>SoftwareName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
