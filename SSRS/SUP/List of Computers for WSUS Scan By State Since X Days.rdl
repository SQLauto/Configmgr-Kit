      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=selucmcen01;Initial Catalog=SELUCMCEN01</ConnectString>
        <IntegratedSecurity>true</IntegratedSecurity>
      </ConnectionProperties>
      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>7cc195a7-afb2-4aec-a521-0512ca879268</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@UpdateSourceID">
            <Value>=Parameters!UpdateSourceID.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@SiteCode">
            <Value>=Parameters!Sitecode.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Statename">
            <Value>=Parameters!Statename.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@days">
            <Value>=Parameters!days.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select
              uss.ResourceID,
              rsys.Name0 as MachineName,
              rsys.Client_Version0 as SMSClientVersion,
              uss.LastWUAVersion as WUAVersion,
			  rsys.User_Domain0+'\'+rsys.User_Name0 as LastLoggedOnUser,
              nullif(uss.LastStatusMessageID&amp;0x0000FFFF, 0) as ErrorStatusID,
              nullif(uss.LastErrorCode, 0) as LastErrorCode,
              sn.StateName as Status,
              uss.LastScanTime,
              uss.LastScanPackageLocation as LastScanPackageLocation,
              uss.LastScanPackageVersion as LastScanPackageVersion,
              statusinfo.MessageStateName as ErrorStatusName  
          from V_UpdateScanStatus uss
          join V_R_System rsys on rsys.ResourceID = uss.ResourceID and isnull(rsys.Obsolete0,0)=0
          join V_SoftwareUpdateSource sus on uss.UpdateSource_ID = sus.UpdateSource_ID
          join V_RA_System_SMSAssignedSites sass on uss.ResourceID = sass.ResourceID
          join V_StateNames sn on sn.TopicType = 501 and sn.StateID=isnull(uss.LastScanState, 0)
         left join V_AdvertisementStatusInformation statusinfo on statusinfo.MessageID=nullif(uss.LastStatusMessageID&amp;0x0000FFFF, 0)
           where sass.SMS_Assigned_Sites0=@SiteCode
		    and sn.StateName=@Statename
			--and (sus.UpdateSource_UniqueID ='{7233E99E-8618-43EC-BC37-85646374E69B}') 
			and sus.UpdateSource_UniqueID =@UpdateSourceID
			and DATEDIFF(Day,uss.LastScanTime,Getdate())&gt;@days
          order by rsys.Name0</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
