            <Value>=Parameters!Days.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText> Select 
              sass.SMS_Assigned_Sites0 as SiteCode,
              site.SiteName as SiteName,
              sn.StateName as Status,
              count(*) as [Total Computers],
              sus.UpdateSource_UniqueID as UpdateSourceID
              from V_UpdateScanStatus  uss
              join V_RA_System_SMSAssignedSites  sass on uss.ResourceID = sass.ResourceID
              join V_R_System  rsys on rsys.ResourceID = uss.ResourceID and isnull(rsys.Obsolete0,0)&lt;&gt;1
              join V_Site  site on sass.SMS_Assigned_Sites0 = site.SiteCode
              join V_StateNames  sn on sn.TopicType = 501 and
              sn.StateID = (case when (isnull(uss.LastScanState, 0)=0 and Left(isnull(rsys.Client_Version0, '4.0'), 1)&lt;'4') then 7 else isnull(uss.LastScanState, 0) end)
              join v_SoftwareUpdateSource  sus on uss.UpdateSource_ID = sus.UpdateSource_ID
              where (SiteCode=@Sitecode or sass.SMS_Assigned_Sites0=@Sitecode)
              and (sus.UpdateSource_ID='' or sus.UpdateSource_UniqueID =@UpdateSourceID)
			  and DATEDIFF(Day,uss.LastScanTime,Getdate())&gt;@Days
              group by sass.SMS_Assigned_Sites0, site.SiteName, sn.StateName, sus.UpdateSource_UniqueID, sus.UpdateSourceName
              order by sus.UpdateSource_UniqueID,sass.SMS_Assigned_Sites0, sn.StateName</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
